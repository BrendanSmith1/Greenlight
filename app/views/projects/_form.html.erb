<%= form_with model: @project do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :company %>
  <%= f.text_field :company %>
  <%= collection_check_boxes(:project, :user_ids, User.all, :id, :email) %>
  <%= f.submit %>
<% end %>
