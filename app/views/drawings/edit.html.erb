<h1>Edit Drawing Page</h1>

<%= form_with model: @drawing, url: project_drawing_path do |f| %>
  <%= f.label :file %>
  <%= f.file_field :file %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :number %>
  <%= f.text_field :number %>
  <%= f.label :revision %>
  <%= f.text_field :revision %>
  <%= f.submit %>
<% end %>
